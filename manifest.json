{
  "manifest_version": 3,
  "name": "Time Tracker XD",
  "version": "1.0",
  "description": "Tracks time spent on websites.",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
