{
  "manifest_version": 3,
  "name": "Time Tracker XD",
  "version": "1.0",
  "description": "Track time spent on websites.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Time Tracker XD"
  },
  "host_permissions": [
    "*://*/*"
  ]
}